{
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": ["eslint:recommended", "eslint-config-prettier"],
  "rules": {
    "strict": ["error", "never"],
    "no-undef": [0],
    "no-unused-vars": [0, {"vars": "local"}]
  },
  "env": {
    "browser": true
  }
}